<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <rule>
        <from>/swagger/(.*)</from>
        <to>/$1</to>
    </rule>
    <rule>
        <from>/admin/homber</from>
        <to>/</to>
    </rule>
    <rule>
        <from>/admin/homber/(.*)</from>
        <to>/$1</to>
    </rule>
    <rule>
        <from>/static/(.*)</from>
        <to>/admin/homber/static/$1</to>
    </rule>
</urlrewrite>